{
  "name": "@prompt-foundry/typescript-sdk",
  "version": "1.0.0",
  "description": "The Typescript SDK for the prompt engineering, prompt management, and prompt testing tool Prompt Foundry.",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "homepage": "https://promptfoundry.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prompt-foundry/typescript-sdk.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "typescript",
    "sdk",
    "prompt",
    "prompt-engineering",
    "prompt-management",
    "prompt-testing"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "lint": "eslint .",
    "prettier": "npm run lint -- --fix",
    "test": "run-s test:*",
    "test:unit": "jest",
    "typecheck": "tsc --noEmit --incremental",
    "watch:build": "tsc -p tsconfig.json -w",
    "watch:test": "npm run test:unit -- --watchAll",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset-hard": "git clean -dfx && git reset --hard && npm i",
    "prepare-release": "run-s reset-hard test doc:html version doc:publish",
    "openapi": "openapi-zod-client https://api.promptfoundry.ai/sdk/openapi -o ./src/openapi/client.ts --export-types --export-schemas && npm run prettier"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-better-mutation": "^1.4.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "gh-pages": "^6.1.1",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "openapi-zod-client": "^1.18.1",
    "prettier": "^3.3.2",
    "prettier-plugin-properties": "^0.3.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@zodios/core": "^10.9.6",
    "axios": "^1.7.2",
    "openai": "^4.50.0",
    "zod": "^3.23.8"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.test.*",
    "!**/test/*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
